{
  "name": "intro-to-MSE",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$path": "src/ReplicatedStorage",
      "$ignoreUnknownInstances": true
    },

    "ServerScriptService": {
      "$ignoreUnknownInstances": true,
      "Server": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "$ignoreUnknownInstances": true,
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    },

    "StarterGui": {
      "$ignoreUnknownInstances": true,
      "$path": "src/starterGui"
    },

    "Workspace": {
      "$ignoreUnknownInstances": true,
      "$properties": {
        "FilteringEnabled": true
      },
      "$path": "src/workspace"
    },
    
    "Lighting": {  
      "$ignoreUnknownInstances": true,
      "$path": "src/lighting",
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$ignoreUnknownInstances": true,
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}
